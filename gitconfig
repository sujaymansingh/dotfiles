[filter "lfs"]
    process = git-lfs filter-process
    required = true
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
[user]
    email = info@sujaymansingh.com
    name = Sujay Mansingh
[color]
    ui = true
[core]
    autocrlf = input
    excludesfile = /Users/sujay/.gitignore
[alias]
    lg = log --pretty='format:%Cgreen%h%Creset %cr [%C(yellow)%cN%Creset] %s'
    diffc = diff --cached
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
[include]
    path = ~/.local-gitconfig
[rebase]
	autoStash = true
[commit]
	verbose = true
[pull]
	ff = only
